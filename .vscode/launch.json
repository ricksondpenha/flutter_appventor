{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DEBUG prod",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_prod.dart",
            "args": ["--flavor", "prod", "-t", "./lib/main_prod.dart"]
        },
        {
            "name": "DEBUG beta",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_beta.dart",
            "args": ["--flavor", "beta", "-t", "./lib/main_beta.dart"]
        },
        {
            "name": "DEBUG dev",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_dev.dart",
            "args": ["--flavor", "dev", "-t", "./lib/main_dev.dart"]
        },
        {
            "name": "MACOS-DEBUG dev",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_dev.dart",
            "args": ["-d", "macos"]
        },
        {
            "name": "PROFILE prod",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": ["--flavor", "prod", "-t", "./lib/main_prod.dart"]
        },
        {
            "name": "RELEASE prod",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": ["--flavor", "prod", "-t", "./lib/main_prod.dart"]
        },
        {
            "name": "WIDGETBOOK",
            "request": "launch",
            "type": "dart",
            "program": "widgetbook/main.dart",
            "args": ["-d", "macos"]
        }
    ]
}